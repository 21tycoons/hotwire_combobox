<%= tag.div class: "hotwire-combobox", data: parent_data do %>
  <%= tag.input **attrs %>

  <%= tag.ul id: listbox_id, hidden: "", role: :listbox, data: { "combobox-target": "listbox" } do |ul| %>
    <% options.each do |option| %>
      <%= tag.li option, role: :option %>
    <% end %>
  <% end %>
<% end %>
